                                    {
                                        <li class="disabled"><a href="" aria-label="Previous"><span aria-hidden="true">&laquo;</span></a></li>
                                    }
                                    else
                                    {
                                        <li><a href="Gebruikers?from=@(from - 2)" aria-label="Previous"><span aria-hidden="true">&laquo;</span></a></li>
                                    }
                                    @for (int i = 1; i <= paginas; i++)
                                    {
                                        if (from / 25 + 1 == i)
                                        {
                                            <li class="active"><a href="Gebruikers?from=@((i - 1) * 25)">@i <span class="sr-only">(current)</span></a></li>
                                        }
                                        else
                                        {
                                            <li><a href="Gebruikers?from=@((i - 1) * 25)">@i <span class="sr-only">(current)</span></a></li>
                                        }

                                    }
                                    @if (paginas == 1 || from + 25 >= gebruikers.Count())
                                    {
