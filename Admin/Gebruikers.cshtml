                            {
                                <td><a href="@Href("Gebruikers?userid=" + gebruikers[i].GebruikerID + "&action=update")">@gebruikers[i].Email</a></td>
                            }   
                                <td>@gebruikers[i].Naam @gebruikers[i].Achternaam</td>
                                <td>@roles</td>
                            </tr>
                        }
                    }
                    else
                    {
