                        <tr>
                            <th>#</th>
                            <th>E-mail</th>
                            <th>Naam</th>
                            <th>Rollen</th>
                        </tr>
                        for(int i = from; i < maxusers + from; i++)
                        {
                            bool isStudent = false;
                            string roles = "";
